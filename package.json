{
  "name": "jakartajs-api",
  "apps_name": "jakartajs-api",
  "version": "1.0.0",
  "description": "JakartaJS API",
  "main": "server.js",
  "scripts": {
    "start": "./node_modules/.bin/nodemon server.js",
    "test": "npm run standard && NODE_ENV=testing ./node_modules/.bin/mocha --exit --reporter spec --timeout 10000 test/*.js",
    "standard": "./node_modules/.bin/standard"
  },
  "author": "Urbanhire Engineer <dev@urbanhire.com>",
  "license": "(ISC OR GPL-3.0)",
  "repository": "git@github.com:jakartajs/jakartajs.git",
  "standard": {
    "globals": [
      "express"
    ],
    "ignore": [
      "/test/*.js",
      "**/*.opts"
    ]
  },
  "pre-commit": [
    "standard"
  ],
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "body-parser": "^1.17.2",
    "celebrate": "^7.0.4",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dont-sniff-mimetype": "^1.0.0",
    "dotenv": "^5.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "extend": "^3.0.1",
    "helmet": "^3.11.0",
    "http-status": "^1.0.1",
    "joi": "^13.1.2",
    "lodash": "^4.13.1",
    "method-override": "^2.3.9",
    "moment": "^2.20.1",
    "mongoose": "^5.0.7",
    "mongoose-unique-validator": "^2.0.0",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "randomstring": "^1.1.5",
    "response-time": "^2.3.2",
    "slugg": "^1.2.1",
    "steed": "^1.1.3",
    "uuid": "^3.1.0",
    "x-xss-protection": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "expect": "^22.1.0",
    "faker": "^4.1.0",
    "mocha": "^5.0.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "should": "^13.2.1",
    "standard": "^11.0.0",
    "supertest": "^3.0.0"
  }
}
